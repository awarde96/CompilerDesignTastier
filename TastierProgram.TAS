// program to read integer values n (n > 0) from the console
// and, for each one, print the sum of the values from 1 to n

program Test {
   int i;
   const int xy;
   array int zzz[3];
   array int xyz[6];
   array int yyy[4][4];

   void main(){
      xy = 3;
      zzz[0] := 8;
      zzz[1] := 9;
      xyz[0] := 7;
      yyy[0][0] := 6;
      i := [0]zzz;
   }
}
